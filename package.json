{
  "name": "@vanhelbergen/design-tokens",
  "description": "This example code is bare-bones to show you what this framework can do. If you have the style-dictionary module installed globally, just cd into this directory and run: ```bash style-dictionary build ```",
  "version": "0.0.3",
  "main": "build/index.js",
  "scripts": {
    "build": "node_modules/.bin/style-dictionary build",
    "clean": "rm -rf build",
    "develop": "yarn build && yarn watch",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "chokidar 'properties/**/*.json' -c 'yarn build'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/allardvanhelbergen/vanhelbergen-tokens.git"
  },
  "keywords": [
    "design-tokens"
  ],
  "author": "Allard van Helbergen",
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/allardvanhelbergen/vanhelbergen-tokens/issues"
  },
  "homepage": "https://github.com/allardvanhelbergen/vanhelbergen-tokens#readme",
  "files": [
    "build",
    "properties"
  ],
  "devDependencies": {
    "chokidar-cli": "^2.0.0",
    "style-dictionary": "^2.8.1"
  }
}
